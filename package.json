{
  "name": "hb-hugo",
  "version": "1.9.0",
  "license": "UNLICENSED",
  "author": "gethugothemes",
  "scripts": {
    "dev": "hugo server",
    "build": "hugo --gc --minify --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --forceSyncStatic",
    "test": "hugo server --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --watch --forceSyncStatic -e production --minify",
    "dev:onetrust": "cd onetrust; hugo server --themesDir ../..",
    "build:onetrust": "cd onetrust; hugo --themesDir ../.. --gc --minify --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --forceSyncStatic",
    "test:onetrust": "cd onetrust; hugo server --themesDir ../.. --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --watch --forceSyncStatic -e production --minify",
    "project-setup": "node ./scripts/projectSetup.js",
    "theme-setup": "node ./scripts/themeSetup.js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "hugo-extended": "^0.122.0",
    "postcss": "^8.4.34",
    "postcss-cli": "^11.0.0"
  }
}
